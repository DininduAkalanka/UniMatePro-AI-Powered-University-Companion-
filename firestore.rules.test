rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules allow anyone with a valid Firebase Auth token
    // to read and write to your database. Use ONLY for development/testing!
    // DO NOT use in production!
    
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
